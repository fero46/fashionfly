<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", backend_scope_contests_path(@scope)  %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_backend_scope_contest_path(@scope) %></li>
    </ul>
  </div>          
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.new", :default => "New")%>  Contest </h2>
    <div class="inner">
      <%= form_for @contest, :url => backend_scope_contests_path(@scope), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>

      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>